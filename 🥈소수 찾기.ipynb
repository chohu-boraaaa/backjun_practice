    "T = int(input())\n",
    "nums = list(map(int, input().split()))\n",
    "\n",
    "sosu = 0\n",
    "for num in nums:\n",
    "    yaksu = []\n",
    "    for i in range(1, num+1):\n",
    "        if num % i == 0:\n",
    "            yaksu.append(i)\n",
    "    if len(yaksu) == 2:\n",
    "        sosu += 1\n",
    "\n",
    "print(sosu)"
   ]
